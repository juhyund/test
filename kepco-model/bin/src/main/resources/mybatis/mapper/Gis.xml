<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.emi.dao.model.GisDao">
	<sql id="query_default_gis">
		SELECT * FROM GIS
	</sql>
	
	<select id="getGisList" resultType="Gis">
		<include refid="query_default_gis"/>
	</select>
	
	<select id="getGis" parameterType="map" resultType="Gis">
		<include refid="query_default_gis"/>
		 WHERE 1=1
		<if test='name != null and name !=""'>
			AND name = #{name}
		</if>
	</select>
</mapper>